{
  "extends": [ "airbnb" ],
  "env": {
    "browser": true,
    "node": true,
  },
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [ "error", "never" ],
    "global-require": 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "import/no-extraneous-dependencies": 0,
    "react/prefer-stateless-function": 0
  }
}
